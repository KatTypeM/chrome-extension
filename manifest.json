{
	"manifest_version": 3,
	"name": "Review URL Later",
	"version": "1.5.0",
	"description": "Use this chrome extension to save URLs that you want to review later.",
	"permissions": [
		"tabs"
		],
	"action": {
		"default_popup": "index.html",
		"default_icon": "icon.png"
	},
	
	"icons": {
		"16": "icon16.png",
		"48": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
	

}